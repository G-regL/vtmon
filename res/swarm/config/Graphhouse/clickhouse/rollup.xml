        <!-- vsphere-historical-datacenter -->
        <pattern>
                <regexp>^vsphere\..*\..*\.datacenter\..*</regexp>
                <function>any</function>
                <retention>
                        <age>0</age>
                        <precision>1800</precision>
                </retention>
                <retention>
                        <age>7882200</age>
                        <precision>7200</precision>
                </retention>
