debug = false
logLevel = "INFO"
defaultEntryPoints = ["http"]

[entryPoints]
  [entryPoints.http]
    address = ":80"
  [entryPoints.api]
    address = ":8080"

[accessLog]
  filePath = "/logs/access.log"
  format = "json"

[retry]

[docker]
  endpoint = "unix:///var/run/docker.sock"
  domain = "<<HOSTIPFQDN>>"
  watch = true
  exposedByDefault = false
  swarmMode = true
  network = "traefik-net"

[api]
  entryPoint = "api"
  dashboard = true
  [api.statistics]
    recentErrors = 100